server {
        listen 8180;
        server_name server_one;
        client_max_body_size 1048576;

        error_page 404 /errors/404.html;

        location / {
            root /home/taung/webserv/www/html;
            index index.html index.htm;
			limit_except;
			autoindex off;
        }
}
server {
        listen 9090;
        server_name server_two;
        client_max_body_size 2097152;
		error_page 500 /errors/500.html;
        error_page 404 /errors/404.html;

        location / {
            root /var/www/html;
            index home.html;
			limit_except GET POST;
			autoindex on;
			proxy_pass http://127.0.0.1:8080/items;
        }

        location /files {
            index asdf.html index.html;
            autoindex on;
        }
}
